{
  "name": "uuid",
  "version": "3.0.1",
  "description": "RFC4122 (v1 and v4) generator",
  "keywords": [
    "uuid",
    "guid",
    "rfc4122"
  ],
  "license": "MIT",
  "bin": {
    "uuid": "./bin/uuid"
  },
  "devDependencies": {
    "mocha": "3.1.2"
  },
  "scripts": {
    "test": "mocha test/test.js"
  },
  "browser": {
    "./lib/rng.js": "./lib/rng-browser.js",
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
