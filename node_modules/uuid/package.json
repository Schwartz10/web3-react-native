    "vm": "vm-browserify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kelektiv/node-uuid.git"
  },
  "react-native": {
    "./lib/rng.js": "./lib/rng-browser.js",
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
