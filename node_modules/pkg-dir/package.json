{
  "name": "pkg-dir",
  "version": "2.0.0",
  "description": "Find the root directory of a Node.js project or npm package",
  "license": "MIT",
  "repository": "sindresorhus/pkg-dir",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "sindresorhus.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
